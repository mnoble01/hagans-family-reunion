{
  "root": "dist/",
  "https_only": true,
  "routes": {
    "/**": "index.html"
  },
  "proxies": {
    "/api/": {
      "origin": "https://hagans-family.herokuapp.com/api"
    }
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "**.js": {
      "Cache-Control": "no-store, no-cache",
      "Access-Control-Allow-Origin": "https://hagans-family.herokuapp.com"
    },
    "**.css": {
      "Cache-Control": "no-store, no-cache",
      "Access-Control-Allow-Origin": "cdn-instance.example.com"
    }
  }
}
